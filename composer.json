    "keywords": [
        "laravel",
        "lumen",
        "aliyun",
        "oss",
        "Object Storage Service",
        "阿里云",
        "对象存储"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "陈恺垣",
            "email": "contact@chenky.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "aliyuncs/oss-sdk-php": "~2.3.0",
        "league/flysystem": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Calchen\\Flysystem\\AliyunOss\\": "src/"
        }
